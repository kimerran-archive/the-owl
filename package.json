{
  "name": "the-owl",
  "version": "1.0.0",
  "description": "Generate api docs based on functional tests",
  "main": "./dist/index.js",
  "scripts": {
    "build": "npm run cleanup && parcel build src/index.js --target node --global theOwl",
    "cleanup": "rm -rf dist/ .cache/",
    "start": "npm run cleanup && parcel watch src/index.js --target node --global theOwl",
    "test": "ava --verbose",
    "test:log-messages": "THE_OWL_LOG_MESSAGES=true npm run test",
    "test:log-redux": "THE_OWL_LOG_REDUX=true npm run test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/leonardosarmentocastro/the-owl.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/leonardosarmentocastro/the-owl/issues"
  },
  "homepage": "https://github.com/leonardosarmentocastro/the-owl#readme",
  "devDependencies": {
    "@babel/core": "^7.4.3",
    "@babel/register": "^7.4.0",
    "ava": "^1.4.1",
    "axios": "^0.18.0",
    "chalk": "^2.4.2",
    "express-mung": "^0.5.1",
    "lodash": "^4.17.11",
    "nodemon": "^1.18.11",
    "parcel-bundler": "^1.12.3",
    "redux": "^4.0.1",
    "rimraf": "^2.6.3",
    "uuid": "^3.3.2"
  },
  "dependencies": {},
  "ava": {
    "files": [
      "test.js ",
      "test-*.js ",
      "test/**/*.js ",
      "**/__tests__/**/*.js ",
      "**/*.test.js",
      "!examples/**/*.js"
    ],
    "require": [
      "./config/test-setup.js"
    ]
  },
  "babel": {
    "presets": [
      "@ava/stage-4"
    ]
  }
}
